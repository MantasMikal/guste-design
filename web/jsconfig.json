{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "target": "es2020",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "Primitive/*": ["./src/components/Primitive/*"],
      "Common/*": ["./src/components/Common/*"],
      "Context/*": ["./src/components/Context/*"],
      "Section/*": ["./src/components/Section/*"],
      "libs/*": ["./src/libs/*"],
      "hooks/*": ["./src/hooks/*"]
    }
  },
  "exclude": ["node_modules"]
}
